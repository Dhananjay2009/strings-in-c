#include <stdio.h>

void printstr(char str[])
{
    int i = 0;
    while (str[i] != '\0') // > ka use != isake stan per kar sakate hai tab \0 n lagane 
                                       //garbage nahi dega
    {
        printf("%c", str[i]);
        i++;
    }
}

int main()
{
    //char str[] = {'k', 'a', 'n', 'h', 'a', '\0'};
    //char str[] = "kanha";
    char str[34];
    gets(str);
    printf("%s\n", str);
    
    //printstr(str);
    puts("using puts: \n");
    return 0;
}
